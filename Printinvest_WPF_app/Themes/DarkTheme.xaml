<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="NavBackgroundColor">#FF2D2D30</Color>
    <Color x:Key="ContentBackgroundColor">#FF1E1E1E</Color>
    <SolidColorBrush x:Key="NavBackgroundBrush" Color="{StaticResource NavBackgroundColor}"/>
    <SolidColorBrush x:Key="ContentBackgroundBrush" Color="{StaticResource ContentBackgroundColor}"/>
    <SolidColorBrush x:Key="NavTextBrush" Color="#EEE" />
    <SolidColorBrush x:Key="TextBrush" Color="White"/>

    <SolidColorBrush x:Key="ButtonBackgroundColor" Color="#FF007ACC" />
    <SolidColorBrush x:Key="ButtonForegroundColor" Color="Black" />
    <SolidColorBrush x:Key="TextBoxBorderColor" Color="#FF2D2D30" />

    <SolidColorBrush x:Key="BorderBrush" Color="#CCCCCC"/>

</ResourceDictionary>